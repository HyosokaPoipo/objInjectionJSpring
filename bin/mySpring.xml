<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"  
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"     
     xsi:schemaLocation="http://www.springframework.org/schema/beans
                         http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                         ">

  <bean id="hisokaClass" class="hisokaSpring.Hisoka">
      <property name="mainJutsu" ref="jutsu1" />
      <property name="secretJutsu" >
      	  <bean class="hisokaSpring.Jutsu">
		      <property name="JutsuName" value="Amaterasu" />
		      <property name="Value" value="97" />
		  </bean>    
      </property>      
      <property name="favoriteJutsu" ref="jutsu3" /> 
      
      
  </bean>
    
  <bean id="jutsu1" class="hisokaSpring.Jutsu">
      <property name="JutsuName" value="FutonNoJutsu" />
      <property name="Value" value="87" />
  </bean>  
  
  <bean id="jutsu3" class="hisokaSpring.Jutsu">
      <property name="JutsuName" value="RinneTenseiNoJutsu" />
      <property name="Value" value="99" />
  </bean>
  
  <alias name="hisokaClass" alias="hisokaClass-alias"/>
  
  </beans>